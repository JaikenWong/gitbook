{
    "title": "Book Notes",
    "author": "JaikenWong",
    "description": "相关技术书籍的读书笔记",
    "plugins": [
        "back-to-top-button",
        "expandable-chapters-small",
        "insert-logo",
        "code",
        "todo",
        "advanced-emoji",
        "emphasize",
        "splitter",
        "tbfed-pagefooter",
        "sectionx",
        "klipse",
        "alerts",
        "popup",
        "custom-favicon",
        "katex",
        "puml@^1.0.1",
        "graph",
        "chart",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "insert-logo": {
            "url": "images/logo.png",
            "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "code": {
            "copyButtons": true
        },
        "github": {
            "url": "https://github.com/JiakenWong"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy JaikenWong 2020",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "favicon": "images/favicon.ico",
        "chart": {
            "type": "c3"
        },
        "anchor-navigation-ex": {
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        }
    },
    "resolutions": {
        "react-dev-utils": "10.1.0"
    }
}